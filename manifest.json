{
    "manifest_version": 3,
    "name": "WASD Nav",
    "version": "1.0",
  
    "description": "WASD Navigation for webpages.",
    "icons": {"48": "icons/border-48.png"},
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }